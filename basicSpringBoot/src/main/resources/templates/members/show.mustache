{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#member}}
        <tr>
            <td>{{id}}</td>
            <td>{{email}}</td>
            <td>{{password}}</td>
        </tr>
    {{/member}}
    </tbody>
</table>
<a href="/articles/{{member.id}}/edit" class="btn btn-primary">Edit</a>
<a href="/members/{{member.id}}/edit" class="btn btn-primary">Edit</a>
<a href="/members">Go to member list</a>

{{>layouts/footer}}